- job:
    name: 'cccp-index'
    description: |
        Managed by Jenkins Job Builder, do not edit manually!
    node: master
    scm:
        - git:
            url: "{{ cccp_index_repo }}"
            skip-tag: True
    triggers:
        - pollscm: "H/10 * * * *" 
    builders:
        - shell: |
            CWD=`pwd`
            cd /opt/cccp-service/jenkinsbuilder
            python cccp_index_reader.py $CWD/index.d

